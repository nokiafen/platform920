<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:id="@+id/b_geFileFromAssets"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="ResourceUtils.geFileFromAssets"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/b_geFileFromRaw"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="ResourceUtils.geFileFromRaw"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/b_geFileToListFromAssets"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="ResourceUtils.geFileToListFromAssets"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/b_geFileToListFromRaw"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="ResourceUtils.geFileToListFromRaw"
        android:textAllCaps="false" />
</LinearLayout>
